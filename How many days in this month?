def Leapyear(y):
    y = ((y%4==0 and y%100!=0)or(y%400==0))
    return (y)


def daysin(m):
    if 1<= m <=12:
        if m in [1,3,5,7,8,10,12]:
            days = 31
        elif m in [4,6,9,11]:
            days = 30
        elif (m==2 and Leapyear(y)):
            days = 29
        else:
            days = 28
        return days
    else:
        print ("enter a correct month number!!")

